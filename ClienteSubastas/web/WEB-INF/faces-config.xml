<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	

    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>clientesubastas.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Portal</managed-bean-name>
        <managed-bean-class>clientesubastas.Portal</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>clientesubastas.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>clientesubastas.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>NuevoCliente</managed-bean-name>
        <managed-bean-class>clientesubastas.NuevoCliente</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MisSubastas</managed-bean-name>
        <managed-bean-class>clientesubastas.MisSubastas</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>VerSubastas</managed-bean-name>
        <managed-bean-class>clientesubastas.VerSubastas</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/IniciarSesion.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Portal.jsp</from-view-id>
        <navigation-case>
            <from-outcome>registrar</from-outcome>
            <to-view-id>/NuevoCliente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>iniciarSesion</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>NuevaSubasta</managed-bean-name>
        <managed-bean-class>clientesubastas.NuevaSubasta</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/MisSubastas.jsp</from-view-id>
        <navigation-case>
            <from-outcome>borrar</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>modificar</from-outcome>
            <to-view-id>/ModificarSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detallesSubasta</from-outcome>
            <to-view-id>/DetallesSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detalles</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refrescar</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>modificarSubasta</from-outcome>
            <to-view-id>/ModificarSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detallesSubasta</from-outcome>
            <to-view-id>/DetallesSubasta.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>ModificarSubasta</managed-bean-name>
        <managed-bean-class>clientesubastas.ModificarSubasta</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DetallesSubasta</managed-bean-name>
        <managed-bean-class>clientesubastas.DetallesSubasta</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/VerSubastas.jsp</from-view-id>
        <navigation-case>
            <from-outcome>mostrarDetalles</from-outcome>
            <to-view-id>/DetallesSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registro</from-outcome>
            <to-view-id>/NuevoCliente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>iniciarSesion</from-outcome>
            <to-view-id>/IniciarSesion.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>misSubastas</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detallesSubasta</from-outcome>
            <to-view-id>/DetallesSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detallesSubasta</from-outcome>
            <to-view-id>/DetallesSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>Cabecera</managed-bean-name>
        <managed-bean-class>clientesubastas.Cabecera</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/NuevoCliente.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>iniciarSesion</from-outcome>
            <to-view-id>/IniciarSesion.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>limpiar</from-outcome>
            <to-view-id>/NuevoCliente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>limpiar</from-outcome>
            <to-view-id>/NuevoCliente.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>IniciarSesion</managed-bean-name>
        <managed-bean-class>clientesubastas.IniciarSesion</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/IniciarSesion.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registro</from-outcome>
            <to-view-id>/NuevoCliente.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sesionIniciada</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/MisSubastas.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>PageDiscordia</managed-bean-name>
        <managed-bean-class>clientesubastas.PageDiscordia</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/NuevaSubasta.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>misSubastas</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ModificarSubasta.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>misSubastas</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/DetallesSubasta.jsp</from-view-id>
        <navigation-case>
            <from-outcome>subastasPublicas</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>misSubastas</from-outcome>
            <to-view-id>/MisSubastas.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevaSubasta</from-outcome>
            <to-view-id>/NuevaSubasta.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/VerSubastas.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
